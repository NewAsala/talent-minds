<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="web_dev_asala.list">
      <field name="name">web_dev_asala list</field>
      <field name="model">web_dev_asala.web_dev_asala</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="web_dev_asala.action_window">
      <field name="name">web_dev_asala window</field>
      <field name="res_model">web_dev_asala.web_dev_asala</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->

    <record model="ir.actions.server" id="web_dev_asala.action_server">
      <field name="name">web_dev_asala server</field>
      <field name="model_id" ref="model_web_dev_asala_web_dev_asala"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>

    <!-- Top menu item -->
    <menuitem name="web_dev_asala" id="web_dev_asala.menu_root"/>
    <!-- menu categories -->
    <menuitem name="Menu 1" id="web_dev_asala.menu_1" parent="web_dev_asala.menu_root"/>
    <menuitem name="Menu 2" id="web_dev_asala.menu_2" parent="web_dev_asala.menu_root"/>
    <!-- actions -->
    <menuitem name="List" id="web_dev_asala.menu_1_list" parent="web_dev_asala.menu_1"
              action="web_dev_asala.action_window"/>
    <menuitem name="Server to list" id="web_dev_asala" parent="web_dev_asala.menu_2"
              action="web_dev_asala.action_server"/>
  </data>
</odoo>